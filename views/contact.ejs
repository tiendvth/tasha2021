<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="icon" href="/admin/images/favicon (2).ico" type="image/ico" />
    <title>TASHA Store | Contact</title>
    <!--FONT AWESOME-->
    <link rel="stylesheet" href="/fontawesome/css/font-awesome.min.css">


    <!--FONTS-->
    <link href="https://fonts.googleapis.com/css?family=Cairo:300,400,700|Josefin+Sans:300i,400,400i,600,700"
          rel="stylesheet">

    <!--Owl Carousel-->
    <link rel="stylesheet" href="/owl-carousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="/owl-carousel/assets/owl.theme.green.min.css">

    <!--BOOTSTRAP CSS-->
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">

    <!--OUR CSS-->
    <link rel="stylesheet" href="/css/general.css">
    <link rel="stylesheet" href="/css/contact.css">

    <!--FOOTER CSS-->
    <link rel="stylesheet" href="/css/footer.css">
    <link rel="stylesheet" href="/css/header.css">

</head>
<body>
<!--START OF HEADER SECTION-->
<%-include('partial/header')%>
<!--END OF HEADER SECTION-->
<!--START OF HOME SECTION-->
<section>
    <div class="row" style="padding-bottom: 100px; padding-top: 100px">
        <div style="width: 100%;height: 500px">
            <div class="col-lg-5">
                <div style="background-color: #f3f3f3;padding: 30px">
                    <h2 class="text-center">Contact Us</h2>
                    <div>
                        <form action="/post/message/contact" method="post" name="" id="validate-form">
                            <input type="text" class="" placeholder="Your Name..." name="name">
                            <input type="email" class="" placeholder="Your Email..." name="email">
                            <textarea placeholder="Your Contact..." cols="30" rows="10" name="content"></textarea>
                            <div class="text-center">
                                <button class="btn btn-submit" name="btn-submit">Send</button>
                            </div>


                        </form>
                    </div>
                </div>

            </div>
            <div class="col-lg-7" style="padding-right: 100px">
                <h2 STYLE="text-align: center">Direct contact</h2>
                <iframe class="embed-responsive-item" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3724.0966890657864!2d105.78009371540224!3d21.028816893151134!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x313455f9bdf0e1c7%3A0x26caee8e7662dd9b!2zRlBUIEFwdGVjaCBIw6AgTuG7mWk!5e0!3m2!1svi!2s!4v1610006568174!5m2!1svi!2s"
                        width="100%" height="450" frameborder="0" style="border:0;" allowfullscreen=""
                        aria-hidden="false" tabindex="0"></iframe>
            </div>
        </div>

    </div>

</section>
<!--END OF HOME SECTION-->

<!--START OF FOOTER SECTION-->
<%-include('partial/footer')%>
<!--END OF FOOTER SECTION-->
<!--.jquery js-->
<script src="/js/jquery.min.js"></script>
<script src="/js/jquery.validate.min.js"></script>
<!--.bootstrap-->
<script src="/bootstrap/js/bootstrap.min.js"></script>
<!--Our JS-->
<script src="/js/validate.js"></script>
</body>
</html>